<?xml version="1.0" encoding="UTF-8"?>
<FindBugsFilter>
    <!-- Ignore any groovy files as they generate Java classes that we don't control -->
    <Match>
        <Source name="~.*\.groovy" />
    </Match>

    <!-- Generated by Hibernate for JPA queries so ignore -->
    <Match>
        <Class name="~.*_"/>
    </Match>
</FindBugsFilter>
